# INPUTS: A in FC; B in FD
# OUTPUTS: C in FE
# BEHAVIOUR: Calculates C = A XOR B; Loops forever

00000: 00 00001 0 0 000 1100 0 1 0 1 1100 0 # R0 = FC
00001: 00 00010 0 1 000 0000 0 1 1 1 0001 0 # R0 = (R0) 
00010: 00 00011 0 0 001 1101 0 1 0 1 1100 0 # R1 = FD
00011: 00 00100 0 1 001 0000 0 1 1 1 0001 0 # R1 = (R1) 
00100: 00 00101 0 0 000 0010 1 1 0 0 0001 0 # R2 = R0
00101: 00 00110 0 0 001 0011 1 1 0 0 0001 0 # R3 = R1
00110: 00 00111 0 0 000 0000 0 1 0 0 0010 0 # R0 = R0 NOR R0
00111: 00 01000 0 0 001 0001 0 1 0 0 0010 0 # R1 = R1 NOR R1
01000: 00 01001 0 0 000 0001 0 1 0 0 0010 0 # R0 = R0 NOR R1
01001: 00 01010 0 0 010 0011 0 1 0 0 0010 0 # R2 = R2 NOR R3
01010: 00 01011 0 0 000 0010 0 1 0 0 0010 0 # R0 = R0 NOR R2
01011: 00 01100 0 0 001 1110 0 1 0 1 1100 0 # R1 = FE
01100: 00 00000	1 1 001 0000 0 0 0 0 1100 0 # (R1) = R0; LOOP 0